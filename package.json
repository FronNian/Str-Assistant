{
  "name": "str-assistant",
  "repository": {
    "type": "git",
    "url": "https://github.com/FronNian/Str-Assistant"
  },
  "publisher": "FronNian/封念",
  "displayName": "Str-Assistant",
  "description": "vsCode字符串转换助手，英文大小写转换，英文字符串头部快速转换，驼峰命名转换",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "main":"./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "str-transform.allToUpper",
        "title": "All To Upper"
      },
      {
        "command": "str-transform.allToLower",
        "title": "All To Lower"
      },
      {
        "command": "str-transform.headToUpper",
        "title": "Head To Upper"
      },
      {
        "command": "str-transform.headToLower",
        "title": "Head To Lower"
      },
      {
        "command": "str-transform.allHeadToUpper",
        "title": "All Head To Upper"
      },
      {
        "command": "str-transform.allHeadToLower",
        "title": "All Head To Lower"
      }
    ]
  },"scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "ts-loader": "^9.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "@vscode/test-electron": "^2.1.5"
  }
}